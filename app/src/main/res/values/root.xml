<?xml version="1.0" encoding="utf-8"?>
<resources>
    <attr name="border_root" format="color"/>
    <attr name="bottom_navigation_item_tint" format="color"/>
    <style name="blueTextReqular12sp400w" parent="textAppearanceRegular12sp400w">
        <item name="textFillColor">@color/blue_from_figma</item>
    </style>
    <style name="buttonMainActivity" parent="Widget.MaterialComponents.Button.UnelevatedButton">
        <item name="android:backgroundTint">@color/white</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_weight">1</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:insetTop">0dp</item>
        <item name="android:insetBottom">0dp</item>
        <item name="android:layout_height">0dp</item>
        <item name="android:layout_marginHorizontal">@dimen/main_layout_padding</item>
        <item name="android:textAllCaps">false</item>
        <item name="iconTint">@color/black</item>
        <item name="iconGravity">textStart</item>
        <item name="iconPadding">@dimen/button_icon_padding</item>
        <item name="cornerRadius">@dimen/button_radius</item>
        <item name="android:textAppearance">@style/textAppearanceMedium22sp500w</item>
    </style>
    <dimen name="main_layout_padding">16dp</dimen>
    <string name="search" translatable="false">Поиск</string>
</resources>