    <style name="textAppearanceRegular13sp400w">
        <item name="android:fontFamily">@font/ys_display_regular</item>
        <item name="android:textFontWeight">400</item>
        <item name="android:textSize">13sp</item>
    </style>
    <style name="textAppearanceRegular11sp400w">
        <item name="android:fontFamily">@font/ys_display_regular</item>
        <item name="android:textFontWeight">400</item>
        <item name="android:textSize">11sp</item>
    </style>
    <style name="textAppearanceRegular16sp400w">
        <item name="fontFamily">@font/ys_display_regular</item>
        <item name="android:textFontWeight">400</item>
        <item name="android:textSize">16sp</item>        
    </style>
    <style name="text_search_list_second">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">16dp</item>
        <item name="android:textColor">?attr/my_view_text_search_second_color</item>
        <item name="android:maxLines">1</item>
        <item name="android:ellipsize">end</item>
        <item name="android:textAppearance">@style/textAppearanceRegular11sp400w</item>
        <item name="android:gravity">center_vertical</item>
    </style>
    <style name="my_toolbars_title_text_appearance">
        <item name="android:textSize">22sp</item>
        <item name="android:textFontWeight">500</item>
        <item name="android:fontFamily">@font/ys_display_medium</item>
    </style>
    <style name="my_toolbars" parent="Widget.MaterialComponents.Toolbar">
        <item name="android:layout_height">56dp</item>
        <item name="android:layout_width">match_parent</item>
        <item name="navigationIcon">@drawable/vector_back_day_night</item>
        <item name="titleTextAppearance">@style/my_toolbars_title_text_appearance</item>
    </style>
    <style name="vector_back_src">
        <item name="android:src">@drawable/vector_back_day_night</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_gravity">center_vertical</item>
        <item name="android:layout_margin">@dimen/main_layout_padding</item>
    </style>
    <style name="imageSettingActivity">
        <item name="android:layout_width">@dimen/setting_image_size</item>
        <item name="android:layout_height">@dimen/setting_image_size</item>
        <item name="android:layout_gravity">end|center_vertical</item>
        <item name="android:layout_marginEnd">@dimen/main_layout_padding</item>
    </style>
    <style name="frameLayoutSettingActivity">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/height_setting_menu</item>
        <item name="minHeight">@dimen/min_height_setting</item>
    </style>
</resources>