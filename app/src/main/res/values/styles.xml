<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="search_edit_box" parent="Widget.MaterialComponents.TextInputLayout.OutlinedBox.Dense">
        <item name="boxStrokeColor">@color/black</item>
        <item name="boxStrokeWidth">0dp</item>

    </style>
    <style name="mys" parent="Widget.MaterialComponents.TextInputLayout.FilledBox.Dense">
        <item name="boxCornerRadiusBottomEnd">25dp</item>
        <item name="boxCornerRadiusBottomStart">25dp</item>
        <item name="boxCornerRadiusTopEnd">25dp</item>
        <item name="boxCornerRadiusTopStart">25dp</item>
        <item name="boxBackgroundColor">@android:color/white</item>
    </style>
    <style name="clear_search_button">
        <item name="android:backgroundTint">@color/black_from_figma</item>
    </style>
    <style name="vector_back_src">
        <item name="android:src">@drawable/vector_light_theme</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_gravity">center_vertical</item>
        <item name="android:layout_margin">@dimen/main_layout_padding</item>
    </style>
    <style name="backgroundMainActivity">
        <item name="android:background">@color/main_background</item>
    </style>
    <style name="titleText">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:fontFamily">@font/ys_display_medium</item>
        <item name="android:textFontWeight">500</item>

        <item name="android:layout_gravity">center_vertical</item>
        <item name="android:textSize">@dimen/button_text_size</item>
    </style>
    <style name="buttonMainActivity">
        <item name="android:backgroundTint">@color/white</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:gravity">center</item>
        <item name="android:textSize">@dimen/button_text_size</item>
        <item name="android:textAllCaps">false</item>
        <item name="iconTint">@color/black</item>
        <item name="iconGravity">textStart</item>
        <item name="iconPadding">@dimen/button_icon_padding</item>
        <item name="cornerRadius">@dimen/button_radius</item>
        <item name="android:fontFamily">@font/ys_display_medium</item>
        <item name="android:textFontWeight">500</item>
    </style>
    <style name="imageSettingActivity">
        <item name="android:layout_width">@dimen/setting_image_size</item>
        <item name="android:layout_height">@dimen/setting_image_size</item>
        <item name="android:layout_gravity">end|center_vertical</item>
        <item name="android:layout_marginEnd">@dimen/main_layout_padding</item>
    </style>
    <style name="textSettingActivity">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:gravity">start|center_vertical</item>
        <item name="android:padding">@dimen/main_layout_padding</item>
        <item name="android:foreground">?attr/selectableItemBackground</item>
        <item name="android:textSize">@dimen/settings_text_size</item>
        <item name="android:textFontWeight">400</item>
        <item name="android:fontFamily">@font/ys_display_regular</item>
    </style>
    <style name="frameLayoutSettingActivity">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/height_setting_menu</item>
        <item name="minHeight">@dimen/min_height_setting</item>

    </style>
</resources>